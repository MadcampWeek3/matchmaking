<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/match_main_guideline_h2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="50dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/match_main_guideline_h3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="230dp" />

    <androidx.cardview.widget.CardView
        android:id="@+id/match_main_personal_cardview"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        app:cardCornerRadius="15dp"
        app:layout_constraintBottom_toTopOf="@+id/match_main_guideline_h3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/match_main_guideline_h2">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/match_main_cardview_guideline_v2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_begin="163dp" />

            <ImageButton
                android:id="@+id/match_main_tier_img"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="15dp"
                android:background="@color/colorWhite"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/match_main_cardview_guideline_v2"
                app:layout_constraintStart_toStartOf="parent"
                app:srcCompat="@drawable/platinum_1_128" />

            <TextView
                android:id="@+id/match_main_id"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="17dp"
                android:fontFamily="@font/notosanskr_medium"
                android:gravity="center_horizontal|center_vertical"
                android:text="닉네임"
                android:textColor="@color/colorBlack"
                android:textSize="20sp"
                app:layout_constraintEnd_toStartOf="@+id/match_main_cardview_guideline_v2"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/match_main_cardview_guideline_v3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_begin="60dp" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/match_main_cardview_guideline_v4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_begin="120dp" />

            <View
                android:id="@+id/match_main_divider1"
                android:layout_width="0dp"
                android:layout_height="1dp"
                android:layout_marginTop="60dp"
                android:background="@color/viewline"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@+id/match_main_cardview_guideline_v2"
                app:layout_constraintTop_toTopOf="parent" />

            <View
                android:id="@+id/match_main_divider2"
                android:layout_width="0dp"
                android:layout_height="1dp"
                android:layout_marginBottom="60dp"
                android:background="@color/viewline"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@+id/match_main_cardview_guideline_v2" />

            <View
                android:id="@+id/match_main_divider3"
                android:layout_width="1dp"
                android:layout_height="wrap_content"
                android:background="@color/viewline"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@+id/match_main_cardview_guideline_v2"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/match_main_tier"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="24dp"
                android:layout_marginLeft="24dp"
                android:text="티어"
                android:fontFamily="@font/notosanskr_black"
                android:textSize="20sp"
                app:layout_constraintBottom_toTopOf="@+id/match_main_cardview_guideline_v3"
                app:layout_constraintStart_toStartOf="@+id/match_main_cardview_guideline_v2"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/match_main_position"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="24dp"
                android:layout_marginLeft="24dp"
                android:text="포지션"
                android:fontFamily="@font/notosanskr_black"
                android:textSize="20sp"
                app:layout_constraintBottom_toTopOf="@+id/match_main_cardview_guideline_v4"
                app:layout_constraintStart_toStartOf="@+id/match_main_cardview_guideline_v2"
                app:layout_constraintTop_toTopOf="@+id/match_main_cardview_guideline_v3" />

            <TextView
                android:id="@+id/match_main_voice"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="24dp"
                android:layout_marginLeft="24dp"
                android:text="보이스톡"
                android:fontFamily="@font/notosanskr_black"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@+id/match_main_cardview_guideline_v2"
                app:layout_constraintTop_toTopOf="@+id/match_main_cardview_guideline_v4" />

            <TextView
                android:id="@+id/match_main_tier_write"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="50dp"
                android:layout_marginLeft="50dp"
                android:fontFamily="@font/notosanskr_medium"
                android:text="플래티넘I"
                android:textColor="@color/colorBlack"
                android:textSize="15sp"
                app:layout_constraintBottom_toTopOf="@+id/match_main_cardview_guideline_v3"
                app:layout_constraintStart_toEndOf="@+id/match_main_tier"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/match_main_position_write"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="45dp"
                android:layout_marginLeft="45dp"
                android:fontFamily="@font/notosanskr_medium"
                android:text="AD"
                android:textColor="@color/colorBlack"
                android:textSize="15sp"
                app:layout_constraintBottom_toTopOf="@+id/match_main_cardview_guideline_v4"
                app:layout_constraintStart_toEndOf="@+id/match_main_position"
                app:layout_constraintTop_toTopOf="@+id/match_main_cardview_guideline_v3" />

            <TextView
                android:id="@+id/match_main_voice_write"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="25dp"
                android:layout_marginLeft="25dp"
                android:fontFamily="@font/notosanskr_medium"
                android:text="ON"
                android:textColor="@color/colorBlack"
                android:textSize="15sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/match_main_voice"
                app:layout_constraintTop_toTopOf="@+id/match_main_cardview_guideline_v4" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>